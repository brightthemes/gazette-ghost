{
  "name": "gazette",
  "description": "Magazine/News Ghost Theme",
  "demo": "https://gazette.bironthemes.com",
  "version": "3.1.0",
  "engines": {
    "ghost": ">=3.0.0",
    "ghost-api": "v3"
  },
  "scripts": {
    "start": "gulp",
    "test": "gscan ."
  },
  "author": {
    "name": "Biron Themes",
    "email": "bironthemes@gmail.com",
    "url": "https://bironthemes.com"
  },
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme"
  ],
  "gpm": {
    "type": "theme",
    "categories": [
      "Minimal",
      "Magazine",
      "Personal Blog"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "fs": "0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "path": "^0.12.7",
    "pixrem": "^5.0.0",
    "postcss-color-rgba-fallback": "^3.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-opacity": "^5.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-vmin": "^3.0.0",
    "postcss-will-change": "^3.0.0",
    "postcss-zindex": "^4.0.1"
  },
  "dependencies": {
    "@tryghost/content-api": "^1.3.10",
    "fitvids": "^2.1.1",
    "font-awesome": "^4.7.0",
    "ghost-search": "^1.1.0",
    "moment": "^2.24.0",
    "vanilla-lazyload": "^12.5.1"
  },
  "config": {
    "posts_per_page": 9,
    "image_sizes": {
      "xxs": {
        "width": 30
      },
      "xs": {
        "width": 100
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 600
      },
      "l": {
        "width": 1000
      },
      "xl": {
        "width": 2000
      }
    }
  }
}
